##check if package is installed
is_installed <- function(pkg_name) {
is.element(pkg_name, installed.packages()[,"Package"])
}
## Example usage:
pkg_name <- "tidyverse"
if (is_installed(pkg_name)) {
cat(pkg_name, "is installed.\n")
} else {
cat(pkg_name, "is not installed.\n")
}
