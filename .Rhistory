arrange(ToothGrowth, dose)
##working with pipes
filtered_toothgrowth <- ToothGrowth %>%
filter(dose==0.5) %>%
group_by(supp) %>%
summarize(mean_len = mean(len, na.rm = T), group="drop")
View(filtered_toothgrowth)
